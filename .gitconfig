[alias]
  gr = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
  st = status
  cm = commit
  stt = status -uno
  di = diff
  cpush = !git push origin `git rev-parse --abbrev-ref HEAD`
  push = cpush
  a = add
  b = branch
  rmt = remote -v
  c = checkout
[color]
  ui = auto
  status = auto
  branch = auto
[diff]
  compactionHeuristic = true
