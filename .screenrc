autodetach on
startup_message off
defscrollback 50000
defmonitor on
caption always "%{= wk} %-w%{=bu dr}%n %t%{-}%+w %= %{=b wk}[%0`] [%l] %{=b wk}%y/%m/%d %{=b wr}%c"
backtick 0 0 0 echo ${ttyname}
termcapinfo xterm* ti@:te@
#term xterm-256color
#term xterm
escape ^z^z
#bindkey "^[[C" next
#bindkey "^[[D" prev
#bindkey "^[OC" next
#bindkey "^[OD" prev
termcapinfo xterm "ks=\E[?1l\E:ku=\E[A:kd=\E[B:kl=\E[D:kr=\E[C:kh=\E[5~:kH=\E[F"

#bindkey "M-[5d" next
#bindkey "M-[5c" prev
bindkey "^[[1;5D" next
bindkey "^[[1;5C" prev
